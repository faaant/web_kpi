{
  "name": "lab4",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --no-error-on-unmatched-pattern --ignore-path .gitignore",
    "lint:fix": "eslint --fix . --no-error-on-unmatched-pattern --ignore-path .gitignore",
    "prettier": "prettier --check . --ignore-path .prettierignore",
    "prettier:fix": "prettier --write . --ignore-path .prettierignore",
    "slint": "stylelint \"**/*.scss\"",
    "slint:fix": "stylelint \"**/*.scss\" --fix"
  },
  "dependencies": {
    "next": "12.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "stylelint-config-sass-guidelines": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.2",
    "prettier": "2.4.1",
    "sass": "^1.43.4",
    "stylelint": "^14.0.1",
    "stylelint-config-standard-scss": "^2.0.1"
  }
}
